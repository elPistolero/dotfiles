! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.color-enabled: true
rofi.color-window: #273238, #273238, #1e2529
rofi.color-normal: #273238, #c1c1c1, #273238, #394249, #ffffff
rofi.color-active: #273238, #80cbc4, #273238, #394249, #80cbc4
rofi.color-urgent: #273238, #ff1844, #273238, #394249, #ff1844

! Fonts {{{
Xft.antialias: true
Xft.hinting:   true
Xft.rgba:      rgb
Xft.hintstyle: hintfull
!Xft.dpi:       220
! }}}
XTerm*faceName: Droid Sans Mono for Powerline
Xterm*faceSize: 11
Xterm*vt100*geometry: 80x60
Xterm*saveLines: 16384
Xterm*loginShell: true
Xterm*charClass: 33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48
Xterm*termName: xterm-color
Xterm*eightBitInput: false
Xterm*rightScrollBar: true
XTerm*utf8: 1
XTerm*renderFont: true
Xterm*ScrollBar: true
UXTerm*faceName: Droid Sans Mono for Powerline
UXterm*faceSize: 11
UXterm*vt100*geometry: 80x60
UXterm*saveLines: 16384
UXterm*loginShell: true
UXterm*charClass: 33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48
UXterm*termName: xterm-color
UXterm*eightBitInput: false
UXterm*rightScrollBar: true
UXterm*ScrollBar: true
UXTerm*utf8: 1
UXTerm*renderFont: true
URxvt.font: xft:Droid Sans Mono for Powerline:size=11
URxvt.letterSpace: -1
URxvt*vt100*geometry: 80x60
URxvt*saveLines: 16384
URxvt*loginShell: true
URxvt*charClass: 33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48
URxvt*termName: screen-256color
URxvt*eightBitInput: false
URxvt*utf8: 1
URxvt*renderFont: true
URxvt.scrollBar: false
URxvt.urgentOnBell: true

!BLK Cursor
